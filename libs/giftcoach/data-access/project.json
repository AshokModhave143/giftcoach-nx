{
  "name": "giftcoach-data-access",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/giftcoach/data-access/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/rollup:rollup",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "libs/giftcoach/data-access/dist",
        "main": "libs/giftcoach/data-access/src/index.ts",
        "tsConfig": "libs/giftcoach/data-access/tsconfig.lib.json",
        "assets": []
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/giftcoach/data-access/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/giftcoach/data-access/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
